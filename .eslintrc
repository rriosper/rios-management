{
  "extends": [
    "react-app",
    "airbnb",
    "plugin:jsx-a11y/recommended",
    "prettier",
    "prettier/react"
  ],
  "plugins": [
    "jsx-a11y",
    "prettier"
  ],
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".ts", ".tsx"] }],
    "prettier/prettier": [
      "error", {
        "semi": true
      }
    ],
    "import/no-unresolved": 0,
    "import/extensions": 0,
    "import/prefer-default-export": 0
  }
}
